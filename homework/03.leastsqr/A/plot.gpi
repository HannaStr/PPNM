 set terminal svg
 set output "radioactive_decay.svg"
 set title "Ordinary Least-Squares Fit (by QR-decomposition) of Radioactive Decay";\
 set xlabel "Time [days]";\
 set ylabel "Per-cent Activity";\
 set tics out
 set key top right;
 set xzeroaxis
 set yzeroaxis
 plot "Out.txt" index 0 using 1:2:3 pt 6 ps 1 with yerrorbars title "Data"\
,"Out.txt" index 1 using 1:2 lt 4 with lines title "Fit"