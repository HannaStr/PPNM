set terminal svg background "white"
set output "linterp.svg"
set title "Linear Interpolation of sin(x)"
set key bottom left
set xlabel "x"
set ylabel "sin(x)"
set xzeroaxis
set yzeroaxis
set tics out
set samples 800
set title "interpolation of sin(x)"
plot 'data.data' using 1:2 with lines lt 6 title "interpolated data"\
,'data.data' using 1:3 with lines lt 3 title "integrated data"\
,'Out.txt' index 0 using 1:2 with points pt 1 title "given data"

